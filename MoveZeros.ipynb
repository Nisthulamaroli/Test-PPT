    "def move_zeros(a):\n",
    "    count=0\n",
    "    for i in range(len(a)):\n",
    "        if a[i]!=0:\n",
    "            a[count]=a[i]\n",
    "            count+=1\n",
    "    while count<len(a):\n",
    "        a[count]=0\n",
    "        count+=1\n",
    "    print(a)\n",
    "            \n",
